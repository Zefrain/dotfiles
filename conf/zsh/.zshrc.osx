_alias()
{
    alias rm="rmtrash"
}

_lib_path()
{
    LD_LIBRARY_PATH=/usr/local/opt/openssl/lib:/usr/local/opt/curl/lib:$LD_LIBRARY_PATH
    export LD_LIBRARY_PATH
}

_incl_path()
{
    C_INCLUDE_PATH=/usr/local/opt/openssl/include:/usr/local/opt/curl/include
    export C_INCLUDE_PATH
}

_init_osx() {
    _alias
    _lib_path
    _incl_path
}


_init_osx
